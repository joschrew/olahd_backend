{
    "settings": {
      "index": {
        "number_of_shards": 5,
        "number_of_replicas": 1,
        "blocks" : {
          "read_only_allow_delete" : "false"
        }
      },
      "analysis": {
        "filter": {
          "german_stop": {
            "type": "stop",
            "stopwords": "stopwords.txt"
          },
          "german_stemmer": {
            "type": "stemmer",
            "language": "light_german"
          },
          "english_stop": {
            "type": "stop",
            "stopwords": "_english_"
          },
          "english_stemmer": {
            "type": "stemmer",
            "language": "english"
          },
          "english_possessive_stemmer": {
            "type": "stemmer",
            "language": "possessive_english"
          }
        },
        "analyzer": {
          "rebuilt_german": {
            "tokenizer": "standard",
            "filter": [
              "lowercase",
              "german_stop",
              "german_normalization",
              "german_stemmer"
            ]
          },
          "rebuilt_english": {
            "tokenizer": "standard",
            "filter": [
              "english_possessive_stemmer",
              "lowercase",
              "english_stop",
              "english_stemmer"
            ]
          }
        }
      }
    },
    "mappings": {
      "runtime": {
        "title2": {
          "type": "keyword",
          "script": {
            "source": "if (!doc['title.title.keyword'].empty) { emit(doc['title.title.keyword'].value) } else if (!doc['structrun.title.title.keyword'].empty){ emit(doc['structrun.title.title.keyword'].get(0)); }"
          }
        },
        "creator2": {
          "type": "keyword",
          "script": {
            "source": "if (!doc['creator_infos.name.keyword'].empty) { emit(doc['creator_infos.name.keyword'].value) } else if (!doc['structrun.creator_infos.name.keyword'].empty){ emit(doc['structrun.creator_infos.name.keyword'].get(0)); }"
          }
        },
        "place2": {
          "type": "keyword",
          "script": {
            "source": "if (!doc['publish_infos.place_publish.keyword'].empty) { emit(doc['publish_infos.place_publish.keyword'].value) } else if (!doc['structrun.publish_infos.place_publish.keyword'].empty){ emit(doc['structrun.publish_infos.place_publish.keyword'].get(0)); }"
          }
        },
        "year2": {
          "type": "long",
          "script": {
            "source": "if (doc['publish_infos.year_publish'].size() != 0) { emit(doc['publish_infos.year_publish'].value); } else if (doc['structrun.publish_infos.year_publish'].size() != 0) { emit(doc['structrun.publish_infos.year_publish'].get(0)); }"
          }
        }
      },
